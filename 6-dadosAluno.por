programa
{
  funcao inicio (){
    cadeia nome
    escreva ("Qual seu nome: ")
    leia (nome)
    inteiro idade
    escreva ("Qual sua idade: ")
    leia (idade)
    real altura
    escreva ("Qual sua altura: ")
    leia (altura)
    escreva (nome + "\n" + "Tem " + idade + " anos" + "\n" + "Possui " + altura + " de altura")
  }
}